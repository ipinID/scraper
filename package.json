{
  "name": "scraper",
  "version": "0.0.1",
  "description": "Web Scraper using NodeJS & Typescript.",
  "main": "index.js",
  "scripts": {
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsc --ext js",
    "build": "tsc",
    "format": "prettier --write .",
    "prepare": "husky install",
    "start": "tsc -w",
    "test": "nodemon"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --stage ng lint ng test",
      "pre-push": "ng build --aot true"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipinID/scraper.git"
  },
  "keywords": [
    "javascript",
    "nodejs",
    "typescript"
  ],
  "author": "ipinID",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ipinID/scraper/issues"
  },
  "homepage": "https://github.com/ipinID/scraper#readme",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "nodemon": "^2.0.21",
    "prettier": "2.8.4",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
